<?xml version="1.0" encoding="utf-8"?>
<!-- Save as res/drawable/calendar_clock.xml -->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="512dp"
    android:height="512dp"
    android:viewportWidth="512"
    android:viewportHeight="512">

    <!-- Common stroke & fill -->
    <!-- Thick outline to match your PNG -->
    <!-- Calendar body (rounded rect) -->
    <path
        android:fillColor="#00000000"
        android:strokeColor="#000000"
        android:strokeWidth="28"
        android:strokeLineJoin="round"
        android:strokeLineCap="round"
        android:pathData="M96,128
                          h240
                          a32,32 0 0 1 32,32
                          v104
                          a32,32 0 0 1 -32,32
                          H96
                          a32,32 0 0 1 -32,-32
                          V160
                          a32,32 0 0 1 32,-32
                          z"/>

    <!-- Calendar top bar (divider) -->
    <path
        android:fillColor="#00000000"
        android:strokeColor="#000000"
        android:strokeWidth="28"
        android:strokeLineCap="square"
        android:pathData="M96,200 H336" />

    <!-- Calendar rings / hanging loops (two small rounded posts) -->
    <path
        android:fillColor="#000000"
        android:pathData="M160,96 a16,16 0 1 1 -0.001,0 z" />
    <path
        android:fillColor="#000000"
        android:pathData="M304,96 a16,16 0 1 1 -0.001,0 z" />

    <!-- Calendar grid squares (3x3 visible grid) -->
    <!-- Row 1 -->
    <path android:fillColor="#000000" android:strokeColor="#000000"
        android:strokeWidth="0" android:pathData="M120,232 h32 v32 h-32 z" />
    <path android:fillColor="#000000" android:pathData="M176,232 h32 v32 h-32 z" />
    <path android:fillColor="#000000" android:pathData="M232,232 h32 v32 h-32 z" />

    <!-- Row 2 -->
    <path android:fillColor="#000000" android:pathData="M120,288 h32 v32 h-32 z" />
    <path android:fillColor="#000000" android:pathData="M176,288 h32 v32 h-32 z" />
    <path android:fillColor="#000000" android:pathData="M232,288 h32 v32 h-32 z" />

    <!-- Row 3 -->
    <path android:fillColor="#000000" android:pathData="M120,344 h32 v32 h-32 z" />
    <path android:fillColor="#000000" android:pathData="M176,344 h32 v32 h-32 z" />
    <path android:fillColor="#000000" android:pathData="M232,344 h32 v32 h-32 z" />

    <!-- Clock circle (overlapping and front) -->
    <path
        android:fillColor="#00000000"
        android:strokeColor="#000000"
        android:strokeWidth="28"
        android:strokeLineJoin="round"
        android:strokeLineCap="round"
        android:pathData="M396,336
                          m-104,0
                          a104,104 0 1,0 208,0
                          a104,104 0 1,0 -208,0" />

    <!-- Clock center dot -->
    <path
        android:fillColor="#000000"
        android:pathData="M396,336 m-8,0 a8,8 0 1,0 16,0 a8,8 0 1,0 -16,0" />

    <!-- Hour hand (points roughly to 12) -->
    <path
        android:fillColor="#000000"
        android:strokeColor="#000000"
        android:strokeWidth="20"
        android:strokeLineCap="round"
        android:pathData="M396,336 L396,272" />

    <!-- Minute hand (points to about 3:30 / down-right) -->
    <path
        android:fillColor="#000000"
        android:strokeColor="#000000"
        android:strokeWidth="20"
        android:strokeLineCap="round"
        android:pathData="M396,336 L444,372" />

    <!-- Decorative underline curve (the swoosh under WORK in your sample) -->
    <path
        android:fillColor="#000000"
        android:strokeColor="#000000"
        android:strokeWidth="14"
        android:strokeLineCap="round"
        android:strokeLineJoin="round"
        android:pathData="M88,392
                          C176,364 272,360 384,392" />
</vector>
